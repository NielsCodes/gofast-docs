{
  "$schema": "https://mintlify.com/schema.json",
  "name": "GoFast Live",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0ea5e9",
    "light": "#0ea5e9",
    "dark": "#0ea5e9",
    "background": {
      "dark": "#000000"
    },
    "anchors": {
      "from": "#0ea5e9",
      "to": "#0ea5e9"
    }
  },
  "theme": "prism",
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:admin@gofast.live"
    }
  ],
  "topbarCtaButton": {
    "name": "Get Started",
    "url": "https://gofast.live"
  },
  "anchors": [
    {
      "name": "CLI",
      "icon": "terminal",
      "url": "https://github.com/gofast-live/gofast-cli"
    },
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://discord.gg/zqXEzmhT"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
